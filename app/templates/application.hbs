{{#if currentUser.user}}
  {{partial 'navbar'}}
{{else}}
  <img src="images/erraroo.png" alt="erraro" id="logo">
{{/if}}
<div class="flash-messages">
  {{#each flashMessages.queue as |flash|}}
    {{flash-message flash=flash messageStyle='bootstrap'}}
  {{/each}}
</div>
<div class="application-outlet">{{outlet}}</div>
<div id="erraroo-debug-helper">
  <button {{action "triggerReoccuringError"}}>reocurring</button>
  <button {{action "triggerNewError"}}>new</button>
</div>
